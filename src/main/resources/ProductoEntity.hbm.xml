<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="modelo.ProductoEntity" table="producto" schema="public" catalog="Minimarket">
        <id name="id" column="id"/>
        <property name="codigo" column="codigo"/>
        <property name="distribuidor" column="distribuidor"/>
        <property name="nombre" column="nombre"/>
        <property name="precio" column="precio"/>
        <property name="unidades" column="unidades"/>
    </class>
</hibernate-mapping>